<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flashcard_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/flashcard_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FlashCardApp">

        <activity
            android:name=".OpenPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Number"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:parentActivityName=".Number">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Number" />
        </activity>

    </application>

</manifest>